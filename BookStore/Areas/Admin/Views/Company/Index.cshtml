

<div class="container">
    <div class="row pt-4 pb-3">
        <div class="col-6">
            <h2 class="text-primary">
                Company List
            </h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Company" asp-action="Upsert" asp-area="Admin" class="btn btn-primary"><i class="bi bi-file-plus"></i>Create New Company</a>
        </div>
    </div>


    <table id="tblData" class="table table-striped ">
        <thead>
            <tr>
                <th>
                    Name
                </th>
                <th>
                    Address
                </th>
                <th>
                    City
                </th>
                <th>
                    State
                </th>
                <th>
                    Phone Number
                </th>
                <th>
                    Actions
                </th>

            </tr>
        </thead>
    </table>

</div>


@section Scripts {
    <script src="~/js/company.js"></script>

}